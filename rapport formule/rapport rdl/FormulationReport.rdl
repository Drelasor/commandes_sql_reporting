left join RndSuite.RndvRqIiLy spiily on formIi.RQ = spiily.RQ and formIi.ICNODE = spiily.ICNODE and formIi.IINODE = spiily.IINODE
left join (select LY, VERSION as LY_VERSION, SEQ as LY_SEQ, COL_ID, 
